{
  "compilerOptions": {
    "outDir": "../server",
    "target": "ESNext",
    "noImplicitAny": false,
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true
  },
  "files": [
    "./index.ts"
  ]
}
